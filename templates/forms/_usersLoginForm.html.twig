<div class="wizard">
    {{ authForm['begin']|raw }}
        <input type="hidden" name="login" value="login">
        <div class="wizard-body pt-3">
            <h3 class="h5 pb-2 ">Or using form below</h3>
            <div class="input-group form-group">
                {{ authForm['email']|raw }}
            </div>
            <div class="input-group form-group">
                {{ authForm['pass']|raw }}
            </div>
        </div>
        <div class="wizard-footer text-right">
            <button class="btn btn-primary" type="submit">Login</button>
        </div>
    {{ authForm['end']|raw }}
</div>