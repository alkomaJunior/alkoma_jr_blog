{{ formOpen|raw }}
    <input type="hidden" name="register" value="register">
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                {{ fistName|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                {{ lastName|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                {{ email|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
               {{ tel|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                {{ pass|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
               {{ confirmPass|raw }}
            </div>
        </div>
    </div>
    <div class="text-right">
        <button class="btn btn-primary" type="submit">S'inscrire</button>
    </div>
{{ formClose|raw }}