{{ usersRegisterForm['begin']|raw }}
    <input type="hidden" name="register" value="register">
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                {{ usersRegisterForm['firstName']|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                {{ usersRegisterForm['lastName']|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                {{ usersRegisterForm['email']|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
               {{ usersRegisterForm['phone']|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                {{ usersRegisterForm['pass']|raw }}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
               {{ usersRegisterForm['confPass']|raw }}
            </div>
        </div>
    </div>
    <div class="text-right">
        <button class="btn btn-primary" type="submit">S'inscrire</button>
    </div>
{{ usersRegisterForm['end']|raw }}